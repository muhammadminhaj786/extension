{
  "manifest_version": 3,
  "name": "Chat Extension",
  "version": "1.0",
  "description": "Extension with working chat panel",
  "permissions": ["sidePanel", "tabs"],
  "background": {
    "service_worker": "background/serviceWorker.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "side_panel": {
    "default_path": "index.html#sidepanel"
  }
}